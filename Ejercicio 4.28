#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int x,suma=0;
    int n1;
    cout<<"Ingrese un numero binario: ";cin>>n1;
    int y;
    for(int i=0;n1!=0;i++){

        x=n1%10;
        y=x*pow(2,i);
        suma=suma+y;
        n1=n1/10;
    }
    cout<<suma;

    return 0;
}
