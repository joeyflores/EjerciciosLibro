#include <iostream>

using namespace std;

int main()
{
    int x;
    int y;
    cout<<"Ingrese la cantidad de valores que desea:";cin>>x;
    int mayor=100000;
    for(int i=1;i<=x;i++){

        cout<<"Ingrese su numero:";cin>>y;
        if(y<mayor){
            mayor=y;
        }

    }
    cout<<"El menor numero es: "<<mayor;
    return 0;
}
